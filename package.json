{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "jest --passWithNoTests --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate:core": "npx plop --plopfile generators/core/plopfile.js",
    "generate:template": "npx plop --plopfile generators/template/plopfile.js",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.3",
    "date-fns": "^3.0.0",
    "eslint-plugin-react": "^7.33.2",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.16",
    "next": "14.1.0",
    "polished": "^4.2.2",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.10.1",
    "react-countup": "^6.5.0",
    "react-datepicker": "^4.24.0",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-multi-carousel": "^2.8.4",
    "react-number-format": "^5.3.1",
    "react-responsive": "^9.0.2",
    "react-spring": "^9.7.3",
    "react-tooltip": "^5.25.1",
    "styled-components": "^6.1.8",
    "web-vitals": "^3.5.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.16",
    "@storybook/nextjs": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/react-datepicker": "^4.19.4",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.2.2",
    "plop": "^4.0.1",
    "prettier": "3.2.5",
    "storybook": "^7.6.16",
    "typescript": "^5"
  }
